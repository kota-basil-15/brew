function deleteCache(){
echo "---------------"
echo "brew cleanup"
echo "---------------"
# delete brew cache
brew cleanup --prune 20

if [[ 0 -eq $? ]]
then
    # 
    echo "deleted brew cache files updated before 20 days"
    for (( i = 0; i < 2; i++ ))
    do
        echo ""
    done
    
    return 0
else
    #
    echo "could not delete brew cache files"
    return 1
fi
}
